<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/style.css" type="text/css">
</head>
<body>
    <div class="views-comtainer">
        <nav class="navbar">

            <!-- Logo -->
            <div class="logo">Neil Dev</div>

            <!-- Navbar -->
            <ul class="nav-links">
                <li class="active"><a href="/"></a>Home</a></li>
                <li><a href="/about">About</a></li>
                <li><a href="/upload-file">Upload File</a></li>
            </ul>
        </nav>

        <div class="list-user">
            <h2>List Users: </h2>

            <table class="table">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Address</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <% dataUsers.forEach((user) => { %>
                        <tr>
                            <td><%= user.firstName %></td>
                            <td><%= user.lastName %></td>
                            <td><%= user.address %></td>
                            <td>
                                <button type="button"><a href="/detail/user/<%= user.id %>">Detail</a></button>
                                <button type="button">Edit</button>
                                <button type="button">Delete</button>
                            </td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>